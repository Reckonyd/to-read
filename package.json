    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "7.1.0",
    "favicons-webpack-plugin": "4.2.0",
    "file-loader": "6.2.0",
    "file-saver": "2.0.2",
    "fork-ts-checker-webpack-plugin": "5.2.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "image-webpack-loader": "7.0.1",
