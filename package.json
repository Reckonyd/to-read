{
  "name": "toread",
  "version": "1.1.0",
  "description": "ToRead helps readers and learners to organize their material and thoughts to a seamless space.",
  "private": true,
  "scripts": {
    "build": "webpack --env production --config webpack.common.js",
    "dev": "webpack-dev-server --env development --config webpack.common.js",
    "lint": "eslint --ext .js,.ts,.vue src functions",
    "lint:css": "stylelint src/**/*.{css,vue}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*.{css,vue}": "stylelint --fix",
    "*.{js,ts,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/thanos.valimitis/todolearn.git"
  },
  "author": "Thanos Valimitis",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/thanos.valimitis/todolearn/issues"
  },
  "homepage": "https://gitlab.com/thanos.valimitis/todolearn#readme",
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@types/file-saver": "2.0.2",
    "@types/lodash": "4.14.168",
    "@types/uuid": "8.3.0",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "@vue/compiler-sfc": "3.0.11",
    "autoprefixer": "10.2.5",
    "axios": "0.21.1",
    "babel-loader": "8.2.2",
    "css-loader": "5.2.1",
    "css-minimizer-webpack-plugin": "2.0.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-vue": "7.9.0",
    "favicons": "6.2.1",
    "favicons-webpack-plugin": "5.0.2",
    "fork-ts-checker-webpack-plugin": "6.2.1",
    "html-webpack-plugin": "5.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.4.1",
    "postcss": "8.2.10",
    "postcss-loader": "5.2.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "puppeteer": "8.0.0",
