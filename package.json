{
  "name": "toread",
  "version": "1.1.0",
  "description": "ToRead helps readers and learners to organize their material and thoughts to a seamless space.",
  "private": true,
  "scripts": {
    "build": "webpack build --node-env=production  --config webpack.common.js",
    "dev": "webpack serve --node-env=development --config webpack.common.js",
    "lint": "eslint --ext .js,.ts,.vue src functions",
    "lint:css": "stylelint src/**/*.{css,vue}",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*.{css,vue}": "stylelint --fix",
    "*.{js,ts,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/thanos.valimitis/to-read.git"
  },
  "author": "Thanos Valimitis",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/thanos.valimitis/todolearn/issues"
  },
  "homepage": "https://gitlab.com/thanos.valimitis/todolearn#readme",
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/file-saver": "2.0.3",
    "@types/uuid": "8.3.1",
    "@types/webpack-env": "1.16.2",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "@vue/compiler-sfc": "3.2.4",
    "autoprefixer": "10.3.1",
    "babel-loader": "8.2.2",
    "css-loader": "6.2.0",
    "css-minimizer-webpack-plugin": "3.0.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-vue": "7.16.0",
    "fork-ts-checker-webpack-plugin": "6.3.2",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "mini-css-extract-plugin": "2.2.0",
    "postcss": "8.3.6",
    "postcss-loader": "6.1.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.3.2",
    "puppeteer": "10.2.0",
    "style-loader": "3.2.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "svg-spritemap-webpack-plugin": "4.2.0",
    "tailwindcss": "2.2.7",
    "ts-loader": "9.2.5",
    "typescript": "4.3.5",
    "vue-loader": "16.5.0",
    "webpack": "5.51.1",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.0.0-rc.0",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "chrome-aws-lambda": "10.1.0",
    "core-js": "3.16.2",
    "file-saver": "2.0.5",
    "puppeteer-core": "10.2.0",
    "vue": "3.2.4",
    "vuex": "4.0.2"
  }
