    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chrome-aws-lambda": "^3.1.1",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.15",
    "puppeteer-core": "3.1.0",
    "uuid": "^8.1.0",
    "vue": "^2.6.11",
    "vuex": "^3.4.0"
  }
