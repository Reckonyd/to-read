    "tailwindcss": "2.1.1",
    "ts-loader": "8.1.0",
    "typescript": "4.2.4",
    "vue-loader": "16.2.0",
    "webpack": "5.32.0",
    "webpack-cdn-plugin": "3.3.1",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "4.0.0-beta.0",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "chrome-aws-lambda": "8.0.2",
